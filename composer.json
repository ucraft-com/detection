{
    "name": "ucraft-com/detection",
    "description": "The Detection component helps application detect visitor's browser details and device type. Determine the geographical location of website visitors based on their IP addresses.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Uc\\Detection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uc\\Detection\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tigran Mesropyan",
            "email": "tiko@ucraft.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.0",
        "torann/geoip": "^3.0",
        "hisorange/browser-detect": "5.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "9.5.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uc\\Detection\\DetectionServiceProvider"
            ]
        }
    }
}
